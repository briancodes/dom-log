{
  "name": "@bcodes/dom-log",
  "version": "1.0.0",
  "description": "Log JSON stringified objects and functions to the DOM - side by side option, terminal styled",
  "author": "briancodes (http://bcodes.io)",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/briancodes/dom-log.git"
  },
  "scripts": {
    "serve": "parcel app/app-index.html  --out-dir app/build --open",
    "build": "microbundle --name domLog",
    "test": "jest",
    "coverage": "jest --coverage",
    "release": "npm run build && np"
  },
  "source": "lib/index.js",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "DOM",
    "console",
    "log",
    "JavaScript"
  ],
  "np": {
    "yarn": false,
    "cleanup": false
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@types/jest": "24.0.15",
    "babel-preset-env": "1.7.0",
    "eslint": "6.0.1",
    "jest": "24.8.0",
    "microbundle": "0.11.0",
    "np": "5.0.3",
    "parcel-bundler": "1.12.3"
  }
}
